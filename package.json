{
  "name": "hacker-native",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:device": "expo run:android --device",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/$1"
    },
    "testMatch": [
      "**/?(*.)+(test).ts?(x)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-reanimated|expo(nent)?|expo-modules-core|@expo(nent)?/.*|@react-native-community/.*|@react-navigation/.*)/)"
    ],
    "collectCoverage": true,
    "coverageReporters": ["text", "lcov", "html"],
    "collectCoverageFrom": [
      "components/**/*.{ts,tsx}",
      "app/**/*.{ts,tsx}",
      "api/**/*.{ts,tsx}",
      "lib/**/*.{ts,tsx}",
      "constants/**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/native": "^6.0.2",
    "@tanstack/react-query": "^5.51.23",
    "date-fns": "^3.6.0",
    "expo": "~51.0.26",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.22",
    "expo-font": "~12.0.9",
    "expo-haptics": "~13.0.1",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.21",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "html-entities": "^2.5.2",
    "i": "^0.3.7",
    "lucide-react-native": "^0.427.0",
    "npm": "^10.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "expo-blur": "~13.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tanstack/eslint-plugin-query": "^5.51.15",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3",
    "@testing-library/react-native": "^12.5.3",
    "@testing-library/jest-native": "^5.4.3",
    "msw": "^2.3.1",
    "whatwg-fetch": "^3.6.20"
  },
  "private": true
}
